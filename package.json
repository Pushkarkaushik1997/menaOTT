{
  "name": "ott",
  "version": "1.0.0",
  "vendor": "Pushkar",
  "description": "A React application for webOS/Tizen",
  "main": "index.js",
  "scripts": {
    "start:webos": "webpack-dev-server --config webpack.webos.config.js --mode development --open",
    "start:tizen": "webpack-dev-server --config webpack.tizen.config.js --mode development --open",
    "build:tizen": "webpack --mode production --config webpack.tizen.config.js  && npm run copy-tizen && tizen package -t wgt -s myTizenProfile -o dist/te/myApp.wgt",
    "build:webos": "webpack --mode production --config webpack.webos.config.js && ares-package -n dist/webos/${npm_package_version} -o dist/webos/${npm_package_version}",
    "copy-tizen": "cp -rp tizen_template/. Dist/tizen",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Pushkar Kaushik",
  "license": "ISC",
